
<div class="row">
  <div class="col l6">
    <%= link_to "Create a Photo", new_photo_path, class: "btn" %>
  </div>
  <div class="col l6">
    <%= link_to "Create a Note", new_note_path, class: "btn" %>
  </div>
</div>

<% notes.each do |note| %>

<div class="row">
  <div class="card file-card col m8 offset-m2">
    <div class="card-content white-text">
      <h3 class="card-title">Note:
        <span><%= link_to note.name, note, class: "link-color" %></span>
      </h3>
    </div>
  </div>
</div>

<% end %>

<% photos.each do |photo| %>

<div class="row">
  <div class="card file-card col m8 offset-m2">
    <div class="card-content white-text">
      <h3 class="card-title">Photo:
      <span><%= link_to photo.name, photo, class: "link-color" %></span>
      </h3>
    </div>
  </div>
</div>
<% end %>
